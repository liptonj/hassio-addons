{
  "name": "ThousandEyes Enterprise Agent",
  "description": "Run ThousandEyes Enterprise Agent for network monitoring and testing",
  "version": "1.0.0",
  "slug": "thousandeyes_agent",
  "init": false,
  "url": "https://github.com/liptonj/hassio-addons/tree/master/thousandeyes",
  "stage": "experimental",
  "arch": [
    "amd64"
  ],
  "startup": "system",
  "boot": "auto",
  "privileged": false,
  "apparmor": false,
  "host_network": false,
  "cap_add": [
    "NET_ADMIN",
    "SYS_ADMIN"
  ],
  "tmpfs": true,
  "map": [
    "data:rw"
  ],
  "options": {
    "account_token": "",
    "agent_hostname": "",
    "inet_mode": "ipv4",
    "memory_limit": "2048",
    "cpu_shares": "1024",
    "log_level": "INFO",
    "log_file_size": "10",
    "proxy_enabled": false,
    "proxy_type": "HTTP",
    "proxy_host": "",
    "proxy_port": 3128,
    "proxy_user": "",
    "proxy_pass": "",
    "proxy_bypass_list": "",
    "custom_dns_enabled": false,
    "custom_dns_servers": [],
    "browserbot_enabled": true,
    "accept_self_signed_certs": false,
    "crash_reports": true,
    "auto_update": true
  },
  "schema": {
    "account_token": "password",
    "agent_hostname": "str?",
    "inet_mode": "list(ipv4|ipv6|dual)?",
    "memory_limit": "str?",
    "cpu_shares": "str?",
    "log_level": "list(DEBUG|INFO|WARNING|ERROR)?",
    "log_file_size": "str?",
    "proxy_enabled": "bool",
    "proxy_type": "list(HTTP|HTTPS|SOCKS5)?",
    "proxy_host": "str?",
    "proxy_port": "int?",
    "proxy_user": "str?",
    "proxy_pass": "password?",
    "proxy_bypass_list": "str?",
    "custom_dns_enabled": "bool",
    "custom_dns_servers": ["str?"],
    "browserbot_enabled": "bool?",
    "accept_self_signed_certs": "bool?",
    "crash_reports": "bool?",
    "auto_update": "bool?"
  }
}

